{
  "minimum_chrome_version": "23",
  "manifest_version": 2,
  "name": "Beagle Term",
  "version": "1.0.0",
  "default_locale": "en",
  "icons": {
    "128": "images/vt128.png",
    "16": "images/vt16.png"
  },
  "description": "Serial terminal emulator.",
  "offline_enabled": true,
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "unlimitedStorage",
    "serial"
  ],
  "app": {
    "background": {
      "scripts": ["js/launcher.js"]
    }
  }
}
